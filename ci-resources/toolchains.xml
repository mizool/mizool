<?xml version="1.0" encoding="UTF8"?>
<toolchains>
    <!--
        This file contains settings for travis-ci.org VMs. We need to install this file by ourselves as the Travis
        team refuses to provide toolchains.xml by default, see https://github.com/travis-ci/travis-ci/issues/2727
    -->
    <toolchain>
        <type>jdk</type>
        <provides>
            <version>1.8</version>
        </provides>
        <configuration>
            <jdkHome>/usr/lib/jvm/java-8-oracle</jdkHome>
        </configuration>
    </toolchain>
</toolchains>